{
	"name": "XPon Center",
	"version": "0.1.1",
	"description": "Your Cloud Central Point",
	"manifest_version": 2,
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {"16": "img/icon/icon16.png",
	          "48": "img/icon/icon48.png",
	          "128":"img/icon/icon128.png"},
	"background": {
	    "scripts": [
	      "js/jquery.min.js",
	      "js/loader.js"
	    ]
    },
    "options_page": "options.html",
    "browser_action": {
		"default_title": "Set this page's color.",
		"default_icon": "img/icon/icon16.png",
		"default_popup": "popup.html"
	},
	"minimum_chrome_version": "20",
	"offline_enabled": true,
	"permissions": [
	    "storage",
		"chrome://favicon/"
	  ],
	"short_name": "XPon Center"
}
